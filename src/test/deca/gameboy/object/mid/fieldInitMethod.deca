class A {
    int a = 20;
    int b = this.initB();
    int c = 10;

    int initB() {
        return 30;
    }

    void check() {
        if (a != 20) {
            println();
        }
        if (b != 30) {
            println();
        }
        if (c != 10) {
            println();
        }
    }
}

{
    A a = new A();
    a.check();

    if (a.a != 60) {
        println();
    }
    if (a.b != 60) {
        println();
    }
    if (a.c != 10) {
        println();
    }
}
