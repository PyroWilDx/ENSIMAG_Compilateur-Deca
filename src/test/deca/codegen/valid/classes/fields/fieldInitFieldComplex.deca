// Description :
//    Test d'une classe où l'initilisation d'un attribut utilise
//    un autre attribut initialisé après
//
// Resultats :
//    0 20

class A {
    int a = this.getB();
    int b = 20;

    int getB() {
        return this.b;
    }

    void displayVars() {
        println(a, " ", b);
    }
}

{
    A a = new A();
    a.displayVars();
}
